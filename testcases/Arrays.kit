class Arrays {
  method void main() {
    S[] arr := new S[10];
    for (int i := 0; i < 10; i := i + 1)
    
    // se i % 3 = 0, salva i in A
      if (i - (i / 3) * 3 = 0) then arr[i] := new A()
      
	// se i % 3 = 1, salva i in B
      else if (i - (i / 3) * 3 = 1) then arr[i] := new B()
      
	// altrimenti salva in S
      else arr[i] := new S();

    int i := 0;
    while (i < 10) {
      arr[i].toString().concat("\n").output();
      i := i + 1
    }
  }
}